{
  "name": "lobo-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 3000",
    "start": "node dist/server/entry.mjs",
    "start:migrate": "npm run migrate-prod || true && npm run start",
    "build": "astro build && nr compile-migrate",
    "build:check": "astro check && nr build",
    "preview": "astro preview",
    "format": "prettier --write ./src/**/*.{astro,ts,js,jsx,tsx,vue}",
    "lint": "eslint --ext .ts,.astro,.js,.jsx,.tsx,.vue ./src",
    "clean": "rimraf dist",
    "astro": "astro",
    "drizzle": "drizzle-kit $npm_config_action --config=./src/server/db/drizzle.config.ts",
    "compile-migrate": "esbuild src/server/db/migrate.ts --bundle --outfile=dist/server/scripts/migrate.mjs --platform=node --format=esm --loader:.node=file --packages=external",
    "migrate-prod": "node dist/server/scripts/migrate.mjs",
    "migrate": "tsx src/server/db/migrate.ts",
    "tsx": "tsx",
    "init-hooks": "simple-git-hooks"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@astrojs/check": "^0.5.6",
    "@astrojs/node": "^8.2.5",
    "@astrojs/react": "^3.3.0",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/vue": "^4.1.0",
    "@auth/core": "^0.18.6",
    "@auth/drizzle-adapter": "^0.9.0",
    "@elysiajs/eden": "^1.0.11",
    "@headlessui/vue": "^1.7.19",
    "@sinclair/typebox": "^0.32.20",
    "@unocss/preset-mini": "^0.59.0",
    "@vee-validate/zod": "^4.12.6",
    "@vueuse/core": "^10.9.0",
    "astro": "^4.6.2",
    "astro-font": "^0.0.77",
    "astro-font-picker": "^0.2.1",
    "astro-icon": "^1.1.0",
    "auth-astro": "^4.1.1",
    "drizzle-orm": "^0.30.7",
    "drizzle-typebox": "^0.1.1",
    "drizzle-zod": "^0.5.1",
    "elysia": "^1.0.13",
    "elysia-http-error": "^1.2.0",
    "i18next": "^23.10.1",
    "keen-slider": "^6.8.6",
    "mande": "^2.0.8",
    "notiwind": "^2.0.2",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "random-js": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-vertical-timeline-component": "^3.6.0",
    "sharp": "^0.33.3",
    "sqids": "^0.3.0",
    "typebox-validators": "^0.3.5",
    "uvcanvas": "^0.2.1",
    "vee-validate": "^4.12.6",
    "vite": "^5.2.9",
    "vue": "^3.4.22",
    "zod": "^3.22.4",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@iconify-json/arcticons": "^1.1.96",
    "@iconify-json/ph": "^1.1.11",
    "@iconify-json/solar": "^1.1.9",
    "@julr/unocss-preset-heropatterns": "^1.0.2",
    "@types/node": "^20.12.7",
    "@types/react-vertical-timeline-component": "^3.3.6",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@unocss/eslint-config": "^0.58.5",
    "@unocss/preset-attributify": "^0.58.5",
    "@unocss/preset-uno": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@unocss/rule-utils": "^0.59.2",
    "drizzle-kit": "^0.20.14",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-vue": "^9.22.0",
    "lint-staged": "^15.2.2",
    "parsel-js": "^1.1.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.10.0",
    "tsx": "^4.7.2",
    "unocss": "^0.59.2",
    "vue-eslint-parser": "^9.4.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{ts,astro,js,jsx,tsx,vue}": "eslint --fix"
  },
  "pnpm": {
    "patchedDependencies": {
      "keen-slider@6.8.6": "patches/keen-slider@6.8.6.patch",
      "astro-font@0.0.77": "patches/astro-font@0.0.77.patch"
    }
  }
}
